{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-node",
        "request": "launch",
        "name": "Vue: Local Dev (.env.development)",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "dev"
        ],
        "env": {
          "NODE_ENV": "development",
          "MODE": "development"
        },
        "console": "integratedTerminal"
      },
      {
        "type": "pwa-node",
        "request": "launch",
        "name": "Vue: Production Dev (.env.production)",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "dev"
        ],
        "env": {
          "NODE_ENV": "production",
          "MODE": "production"
        },
        "console": "integratedTerminal"
      }
    ]
  }
  